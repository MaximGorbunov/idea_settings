<keymap version="1" name="Default copy" parent="$default">
  <action id="$LRU">
    <keyboard-shortcut first-keystroke="shift ctrl alt r" />
  </action>
  <action id="ActivateFavoritesToolWindow" />
  <action id="ActivateGradleToolWindow">
    <keyboard-shortcut first-keystroke="alt g" />
  </action>
  <action id="ActivateMavenProjectsToolWindow">
    <keyboard-shortcut first-keystroke="alt m" />
  </action>
  <action id="ActivateProjectToolWindow">
    <keyboard-shortcut first-keystroke="alt p" />
  </action>
  <action id="Back">
    <mouse-shortcut keystroke="button4" />
    <keyboard-shortcut first-keystroke="shift ctrl alt z" />
  </action>
  <action id="ChooseRunConfiguration">
    <keyboard-shortcut first-keystroke="shift alt f10" />
    <keyboard-shortcut first-keystroke="alt r" />
  </action>
  <action id="Editor LRU">
    <keyboard-shortcut first-keystroke="shift ctrl alt r" />
  </action>
  <action id="ExternalSystem.RefreshAllProjects">
    <keyboard-shortcut first-keystroke="shift ctrl back_slash" />
  </action>
  <action id="Git.Branches">
    <keyboard-shortcut first-keystroke="alt b" />
  </action>
  <action id="LocalHistory">
    <keyboard-shortcut first-keystroke="shift ctrl alt open_bracket" />
  </action>
  <action id="LocalHistory.ShowHistory">
    <keyboard-shortcut first-keystroke="alt l" />
  </action>
  <action id="OpenFile">
    <keyboard-shortcut first-keystroke="shift ctrl o" />
  </action>
  <action id="ToggleDistractionFreeMode">
    <keyboard-shortcut first-keystroke="shift ctrl alt add" />
  </action>
  <action id="TogglePresentationMode">
    <keyboard-shortcut first-keystroke="shift ctrl alt back_quote" />
  </action>
  <action id="hg4idea.branches">
    <keyboard-shortcut first-keystroke="alt h" />
  </action>
</keymap>